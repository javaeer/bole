<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.net.yunlou.bole.mapper.SystemMapper">
    <resultMap id="systemConfigResultMap" type="cn.net.yunlou.bole.entity.SystemConfig">
        <id property="configKey" column="config_key"/>
        <result property="configValue" column="config_value"/>
        <result property="configDesc" column="config_desc"/>
        <result property="createdById" column="created_by_id"/>
        <result property="createdAt" column="created_at"/>
        <result property="updatedById" column="updated_by_id"/>
        <result property="updatedAt" column="updated_at"/>

        <association property="createdBy" column="created_by_id"
                     select="cn.net.yunlou.bole.mapper.UserMapper.selectById"/>

        <association property="updatedBy" column="updated_by_id"
                     select="cn.net.yunlou.bole.mapper.UserMapper.selectById"/>

    </resultMap>
</mapper>