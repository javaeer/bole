<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.net.yunlou.bole.mapper.UserRoleMapper">

    <select id="selectListRight" resultType="cn.net.yunlou.bole.entity.Role">
        select r.* from t_role as r
        left join t_user_role as et on et.role_id = r.id
        where et.user_id = #{et.userId}
    </select>
</mapper>